@page
@model Inlämning.Pages.Admin.ManageUsersModel
@{
}

<div class="text-center">
    <h1> ManageUsersModel!</h1>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model._Users[0].Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model._Users[0].UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model._Users[0].PhoneNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model._Users[0].Email)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model._Users)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PhoneNumber)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

























